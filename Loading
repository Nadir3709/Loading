-- Nadir Hub Loading

local LoadingScreenNew = Instance.new("ScreenGui")
local LoadingFrame = Instance.new("Frame")
local Bar = Instance.new("Frame")
local UICorner = Instance.new("UICorner")
local Inside = Instance.new("Frame")
local UICorner_2 = Instance.new("UICorner")
local LoadingAndCheckingGame = Instance.new("TextLabel")

--Properties:

LoadingScreenNew.Name = "Loading Screen New"
LoadingScreenNew.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
LoadingScreenNew.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

LoadingFrame.Name = "Loading Frame"
LoadingFrame.Parent = LoadingScreenNew
LoadingFrame.BackgroundColor3 = Color3.fromRGB(36, 36, 36)
LoadingFrame.BorderColor3 = Color3.fromRGB(36, 36, 36)
LoadingFrame.BorderSizePixel = 0
LoadingFrame.Position = UDim2.new(0.406719923, 0, 0.3975721, 0)
LoadingFrame.Size = UDim2.new(0, 297, 0, 134)

Bar.Name = "Bar"
Bar.Parent = LoadingFrame
Bar.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Bar.Position = UDim2.new(0.0235690232, 0, 0.522388101, 0)
Bar.Size = UDim2.new(0, 284, 0, 22)

UICorner.Parent = Bar

Inside.Name = "Inside"
Inside.Parent = Bar
Inside.BackgroundColor3 = Color3.fromRGB(0, 255, 127)
Inside.BorderColor3 = Color3.fromRGB(0, 255, 127)
Inside.Size = UDim2.new(0, 0, 1, 0)

UICorner_2.Parent = Inside

LoadingAndCheckingGame.Name = "Loading And Checking Game"
LoadingAndCheckingGame.Parent = LoadingScreenNew
LoadingAndCheckingGame.BackgroundColor3 = Color3.fromRGB(36, 36, 36)
LoadingAndCheckingGame.BorderColor3 = Color3.fromRGB(36, 36, 36)
LoadingAndCheckingGame.BorderSizePixel = 0
LoadingAndCheckingGame.Position = UDim2.new(0.411094934, 0, 0.442404985, 0)
LoadingAndCheckingGame.Size = UDim2.new(0, 283, 0, 33)
LoadingAndCheckingGame.Font = Enum.Font.SourceSans
LoadingAndCheckingGame.Text = "Checking Game"
LoadingAndCheckingGame.TextColor3 = Color3.fromRGB(255, 255, 255)
LoadingAndCheckingGame.TextScaled = true
LoadingAndCheckingGame.TextSize = 14.000
LoadingAndCheckingGame.TextStrokeColor3 = Color3.fromRGB(255, 255, 255)
LoadingAndCheckingGame.TextWrapped = true

-- Scripts:

local function TRRS_fake_script() -- LoadingFrame.Script Hub 
	local script = Instance.new('LocalScript', LoadingFrame)

	wait(18)
	loadstring(game:HttpGet("https://raw.githubusercontent.com/Nadir3709/Huubb/main/Script%20Huubb"))()
end
coroutine.wrap(TRRS_fake_script)()
local function HKILY_fake_script() -- LoadingFrame.Visible 
	local script = Instance.new('LocalScript', LoadingFrame)

	wait(22)
	script.Parent.Visible=false
end
coroutine.wrap(HKILY_fake_script)()
local function RWHYK_fake_script() -- Inside.LocalScript 
	local script = Instance.new('LocalScript', Inside)

	script.Parent:TweenSize(UDim2.new(1,0,1,0),"InOut","Sine",22)
end
coroutine.wrap(RWHYK_fake_script)()
local function PSJB_fake_script() -- LoadingAndCheckingGame.Checking And Loading Text 
	local script = Instance.new('LocalScript', LoadingAndCheckingGame)

	while true do
		script.Parent.Text = "Checking Game" -- 1
		wait(1)
		script.Parent.Text = "Checking Game." -- 2
		wait(1)
		script.Parent.Text = "Checking Game.." -- 3
		wait(1)
		script.Parent.Text = "Checking Game..." -- 4
		wait(1)
		script.Parent.Text = "Checking Game..." -- 5
		wait(1)
		script.Parent.Text = "Checking Game" -- 6
		wait(1)
		script.Parent.Text = "Checking Game." -- 7
		wait(1)
		script.Parent.Text = "Checking Game.." -- 8
		wait(1)
		script.Parent.Text = "Checking Game..." -- 9
		wait(1)
		script.Parent.Text = "Checking Game..." -- 10
		wait(1)
		script.Parent.Text = "Game Found!"
		wait(2)
		script.Parent.Text = "Loading Script" -- 1
		wait(1)
		script.Parent.Text = "Loading Script." -- 2
		wait(1)
		script.Parent.Text = "Loading Script.." -- 3
		wait(1)
		script.Parent.Text = "Loading Script..." -- 4
		wait(1)
		script.Parent.Text = "Loading Script..." -- 5
		wait(1)
		script.Parent.Text = "Loading Script" -- 6
		wait(1)
		script.Parent.Text = "Loading Script." -- 7
		wait(1)
		script.Parent.Text = "Loading Script.." -- 8
		wait(1)
		script.Parent.Text = "Loading Script..." -- 9
		wait(1)
		script.Parent.Text = "Loading Script..." -- 10
		wait(1)
	end
end
coroutine.wrap(PSJB_fake_script)()
local function THBEX_fake_script() -- LoadingAndCheckingGame.Visible 
	local script = Instance.new('LocalScript', LoadingAndCheckingGame)

	wait(22)
	script.Parent.Visible=false
end
coroutine.wrap(THBEX_fake_script)()
