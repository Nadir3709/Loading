-- Loading...
local LoadingScreen = Instance.new("ScreenGui")
local LoadingFrame = Instance.new("Frame")
local TextLabel = Instance.new("TextLabel")

--Properties:

LoadingScreen.Name = "LoadingScreen"
LoadingScreen.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
LoadingScreen.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

LoadingFrame.Name = "Loading Frame"
LoadingFrame.Parent = LoadingScreen
LoadingFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
LoadingFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
LoadingFrame.BorderSizePixel = 100
LoadingFrame.Size = UDim2.new(1, 0, 1, 0)

TextLabel.Parent = LoadingFrame
TextLabel.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
TextLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
TextLabel.Position = UDim2.new(0.357894719, 0, 0.41574806, 0)
TextLabel.Size = UDim2.new(0, 476, 0, 106)
TextLabel.Font = Enum.Font.SourceSans
TextLabel.Text = "Loading Script"
TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.TextScaled = true
TextLabel.TextSize = 94.000
TextLabel.TextStrokeColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.TextWrapped = true

-- Scripts:

local function BUTE_fake_script() -- TextLabel.LocalScript 
	local script = Instance.new('LocalScript', TextLabel)

	while true do
		script.Parent.Text = "Loading Script"
		wait(0.25)
		script.Parent.Text = "Loading Script."
		wait(0.25)
		script.Parent.Text = "Loading Script.."
		wait(0.25)
		script.Parent.Text = "Loading Script..."
		wait(0.25)
	end
end
coroutine.wrap(BUTE_fake_script)()
local function LPNDUAL_fake_script() -- LoadingFrame.LocalScript 
	local script = Instance.new('LocalScript', LoadingFrame)

	wait(4)
	script.Parent.Visible=false
end
coroutine.wrap(LPNDUAL_fake_script)()
local function CUTUT_fake_script() -- LoadingFrame.LocalScript 
	local script = Instance.new('LocalScript', LoadingFrame)
	loadstring(game:HttpGet("https://raw.githubusercontent.com/Nadir3709/Hub/main/Script%20Hubbb"))()
	wait()
	
end
coroutine.wrap(CUTUT_fake_script)()
